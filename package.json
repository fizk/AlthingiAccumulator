{
  "name": "althingi-accumulator",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "tsc:watch": "npm run tsc -- -w",
    "process": "node ./dist/index.js",
    "process:watch": "nodemon ./dist/index.js",
    "test": "jest",
    "compile": "tsc && node ./dist/index.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "@types/amqplib": "^0.5.10",
    "@types/elasticsearch": "^5.0.35",
    "@types/mongodb": "^3.1.20",
    "amqplib": "^0.5.3",
    "forever": "^0.15.3",
    "mongodb": "^3.1.13",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "babel-jest": "^24.5.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^23.10.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "__tests__"
    ],
    "testPathIgnorePatterns": [
      "__tests__/Mongo.ts",
      "__tests__/Server.ts"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/@types/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
